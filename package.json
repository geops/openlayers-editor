    "babel-loader": "^7.1.0",
    "babel-preset-es2015": "^6.24.1",
    "css-loader": "^0.28.7",
    "file-loader": "^1.1.5",
    "husky": "^0.14.3",
    "image-webpack-loader": "^3.3.0",
    "jsdoc-export-default-interop": "^0.3.1",
    "lint-staged": "^6.1.1",
    "openlayers": "^4.0.1",
    "prettier": "^1.8.2",
    "style-loader": "^0.19.0",
    "uglifyjs-webpack-plugin": "^1.0.1",
    "url-loader": "^0.6.2",
    "webpack": "~3.9.0",
    "webpack-dev-server": "^2.4.2"
  },
  "dependencies": {
    "jsdoc": "^3.4.3",
    "mustache": "^2.3.0",
    "optipng-bin": "^4.0.0",
    "pngquant": "^1.3.0",
